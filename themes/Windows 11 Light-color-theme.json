	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#028009",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "comment.block.preprocessor",
			"settings": {
				"foreground": "#AAAAAA",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#028009"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#0E00FF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "keyword.operator.logical",
			"settings": {
				"foreground": "#0E00FF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#262626",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.support.type",
				"support.type.property-name",
				"punctuation.definition.decorator",
			],
			"settings": {
				"foreground": "#262626",
				"fontStyle": ""
			}
		},
		
		{
			"scope": [
				"support.type.exception.python",
			],
			"settings": {
				"foreground": "#C40000",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"storage.type",
				"support.type",
			],
			"settings": {
				"foreground": "#0E00FF",
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language",
				"punctuation.definition.decorator.python",
				"entity.name.function.decorator.python",
				"variable.parameter.function.language.special.self",
				"support.type.python",
			],
			"settings": {
				"foreground": "#C05A28",
				"fontStyle": "",
			}
		},
		{
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#C40000",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.class",
			],
			"settings": {
				"foreground": "#C40000",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"constant.character.format.placeholder.other",
			],
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": "punctuation",
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
				"string.quoted.docstring",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#0E00FF"
			}
		},
		{
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#262626",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity",
			],
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"meta.property-name",
			],
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#028009"
			}
		},
		{
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.error",
			"settings": {
				"foreground": "#C05A28"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#262626"
			}
		},
		{
			"scope": "meta.link",
			"settings": {
				"foreground": "#0E00FF"
			}
		},
		{
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#AA04F9"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#C40000"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.traceback",
			"settings": {
